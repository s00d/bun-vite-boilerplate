{
  "name": "bun-vite-boilerplate",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build:client": "bun x vite build",
    "build:ssr": "bun x vite build --mode ssr",
    "build": "bun run build:client && bun run build:ssr",
    "generate": "bun run scripts/generateStaticPages.ts",
    "dev": "concurrently --kill-others-on-fail --success first -n bun,vite -c blue,green \"cross-env NODE_ENV=development bun --hot run src/server/index.ts\" \"cross-env NODE_ENV=development vite\"",
    "start": "cross-env NODE_ENV=production bun run src/server/index.ts",
    "test": "vitest run",
    "lint": "bun x biome check .",
    "lint:format": "bun x biome format .",
    "lint:format:fix": "bun x biome format . --write",
    "loadtest:login": "bun run scripts/loadtest.ts",
    "loadtest:homepage": "bun run scripts/homepagetest.ts",
    "artillery:login": "npx artillery run scripts/artillery-login.yml",
    "artillery:homepage": "npx artillery run scripts/artillery-homepage.yml"
  },
  "dependencies": {
    "@bogeychan/elysia-etag": "^0.1.0",
    "@bogeychan/elysia-logger": "^0.1.8",
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/cron": "^1.2.0",
    "@elysiajs/html": "^1.2.0",
    "@elysiajs/server-timing": "^1.2.1",
    "@elysiajs/static": "^1.2.0",
    "@elysiajs/swagger": "^1.2.2",
    "@labzzhq/compressor": "^1.1.1",
    "@sinclair/typebox": "^0.34.33",
    "@tailwindcss/postcss": "^4.1.4",
    "@tailwindcss/vite": "^4.1.4",
    "@vue/server-renderer": "^3.5.13",
    "@vueuse/head": "^2.0.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.9.1",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.0",
    "drizzle-orm": "^0.42.0",
    "drizzle-typebox": "^0.3.2",
    "elysia": "^1.2.25",
    "elysia-connect-middleware": "^0.0.5",
    "elysia-i18next": "^4.0.3",
    "elysia-ip": "^1.0.8",
    "elysia-rate-limit": "^4.3.0",
    "elysia-xss": "^1.0.3",
    "i18next": "^25.0.0",
    "i18next-fs-backend": "^2.6.0",
    "mmdb-lib": "^2.1.1",
    "pinia": "^3.0.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/autocannon": "^7.12.6",
    "@types/bun": "latest",
    "@types/mime-types": "^2.1.4",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/tsconfig": "^0.7.0",
    "autocannon": "^8.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.2",
    "esbuild-plugin-vue": "^0.2.4",
    "sass-embedded": "^1.86.3",
    "unplugin-starter": "^0.1.0",
    "unplugin-vue": "^6.1.0",
    "vite": "^6.3.2",
    "vite-plugin-vue-devtools": "^7.7.3",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.0.21"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
